{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
  "baseUrl": ".",
  "paths": {
    "@/*": ["./src/*"],
    "$modules/*": ["./src/modules/*"],
    "$context/*": ["./src/core/context/*"]
  },
  "plugins": [
    {
      "name": "next"
    }
  ]
}
